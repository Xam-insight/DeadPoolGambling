				<Anchors>
					<Anchor point="TOP" relativePoint="BOTTOM" x="0" y="5"/>
				</Anchors>
				<Scripts>
					<OnMouseDown>
						if(self:GetParent().canMove) then
							self:GetParent().moving = true
							self:GetParent():SetFrameStrata("DIALOG")
							dpSetHelpTipFramesStrata("TOOLTIP")
							self:GetParent():StartSizing("BOTTOM")
						end
					</OnMouseDown>
					<OnMouseUp>
						if( self:GetParent().canMove and self:GetParent().moving ) then
							self:GetParent().moving = false
							self:GetParent():StopMovingOrSizing()
							self:GetParent():SetFrameStrata("BACKGROUND")
							dpSetHelpTipFramesStrata("LOW")
							DeadpoolWindow[Deadpool_WindowsOptions]["DeadpoolHeight"] = self:GetParent():GetHeight()
							Deadpool_UpdateScrollFrame(DeadpoolScrollFrame)
							deadpoolSaveWindowPosition()
						end
					</OnMouseUp>
					<OnEnter>
						DeadpoolFrameTemplate_OnEnter(self:GetParent())
					</OnEnter>
					<OnLeave>
						DeadpoolFrameTemplate_OnLeave(self:GetParent())
					</OnLeave>
