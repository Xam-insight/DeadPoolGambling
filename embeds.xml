	<CheckButton name="DeadpoolLockTemplate" virtual="true">
		<Size x="16" y="16"/>
		<Anchors>
			<Anchor point="RIGHT" relativeKey="$parent.MaximizeMinimizeFrame" relativePoint="LEFT" x="0" y="1"/>
		</Anchors>
		<Scripts>
			<OnLoad>
				local locked = DeadpoolWindow[Deadpool_WindowsOptions][self:GetParent():GetAttribute("LockOptionParam")]
				self:SetChecked(locked)
				self:GetParent().canMove = not locked
				DPEnableMouse(self:GetParent(), not locked)
				if self:GetParent().SizeDown then
					if locked then
						self:GetParent().SizeDown:Hide()
					else
						self:GetParent().SizeDown:Show()
					end
				end
			</OnLoad>
			<OnClick>
				local locked = self:GetChecked()
				self:GetParent().canMove = not locked
				DPEnableMouse(self:GetParent(), not locked)
				self:GetParent().alphaFunc()
				if self:GetParent().SizeDown then
					if locked then
						self:GetParent().SizeDown:Hide()
					else
						self:GetParent().SizeDown:Show()
					end
				end
				DeadpoolWindow[Deadpool_WindowsOptions][self:GetParent():GetAttribute("LockOptionParam")] = locked
				Deadpool_PlaySound(SOUNDKIT.UI_70_ARTIFACT_FORGE_APPEARANCE_LOCKED, "SFX", true)
			</OnClick>
			<OnEnter>
				DeadpoolIconButtonEnter(self)
			</OnEnter>
			<OnLeave>
				DeadpoolIconButtonLeave(self)
			</OnLeave>
		</Scripts>
